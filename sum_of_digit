/*package whatever //do not write package name here */

import java.io.*;
import java.util.*;

class sum_of_digit {
	public static void main (String[] args) {
	    Scanner in=new Scanner(System.in);
	    int n=in.nextInt();
	    int sum=0,rem=0;
	    while(n>0){
	        rem=n%10;
	        sum +=rem;
	        n =n/10;
	    }
	    System.out.println(" Sum = "+sum);
	}
}
